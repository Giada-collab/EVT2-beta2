                  <ref name="tei_idno"/>
                  <ref name="tei_model.ptrLike"/>
                  <ref name="tei_editor"/>
            
            
            
               </choice>
            </zeroOrMore>
        
      
            <ref name="tei_imprint"/>
      
            <zeroOrMore>
               <choice>
                  <ref name="tei_imprint"/>
                  <ref name="tei_extent"/>
                  <ref name="tei_biblScope"/>
               </choice>
            </zeroOrMore>
      
         </group>
         <ref name="tei_att.global.attributes"/>
         <empty/>
      </element>
